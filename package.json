  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "prepack": "npm run build",
    "build": "mkdir dist; cp ./src/openssl-ha.node ./dist && cp ./src/openssl-ha.ts ./dist/index.ts",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Nhahan/openssl-ha.git"
  },
  "homepage": "https://github.com/Nhahan/openssl-ha",
  "bugs": {
    "url": "https://github.com/Nhahan/openssl-ha/issues",
    "email": "quevirus324@gmail.com"
  },
  "keywords": [
    "openssl",
    "encrypt",
    "decrypt",
    "hashing",
    "aes-256",
    "hs-256",
    "hmac",
    "security",
    "aes256",
    "hs256",
    "openssl-js"
  ],
  "author": "Nhahan",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.15.0",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.7.13",
    "babel-loader": "^8.2.2",
    "ignore-loader": "^0.1.2",
    "jest": "^27.2.5",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.4.4",
    "typescript": "^4.4.2",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0"
  }
